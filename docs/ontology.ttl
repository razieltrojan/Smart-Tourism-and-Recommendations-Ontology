#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Andalusian
                :Italian
                :Japanese
                :Mediterranean
                :Mexican
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Beach
                :Cultural
                :Momument
                :Museum
                :Park
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :BeachHotel
                :Business
                :Luxury
                :Rural
                :Tourist
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Bus
                :Streetcar
                :Subway
                :Taxi
                :bicycle
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :CulturalEvent
                :Gastronomic
                :Musical
                :Sporting
                :Technological
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Barcelona
                        :Madrid
                        :Toledo
                        :Valencia
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :LocBarcelona
                        :LocMadrid
                        :LocToledo
                        :LocValencia
                      )
] .


