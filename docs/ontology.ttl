#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasAttractionType
:hasAttractionType rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isAttractionTypeOf ;
                   rdfs:domain :Attraction ;
                   rdfs:range :TypeAttraction ;
                   rdfs:comment "Attraction type"@en ;
                   rdfs:label "hasAttractionType"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasCityOffer
:hasCityOffer rdf:type owl:ObjectProperty ;
              owl:inverseOf :isInCity ;
              rdfs:domain :City ;
              rdfs:range :Offers ;
              rdfs:comment "inverse of isInCity"@en ;
              rdfs:label "hasCityOffer"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasEventType
:hasEventType rdf:type owl:ObjectProperty ;
              owl:inverseOf :isEventTypeOf ;
              rdfs:domain :Event ;
              rdfs:range :TypeEvent ;
              rdfs:comment "Type of event"@en ;
              rdfs:label "hasEventType"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasHotelType
:hasHotelType rdf:type owl:ObjectProperty ;
              owl:inverseOf :isHotelTypeOf ;
              rdfs:domain :Hotel ;
              rdfs:range :TypeHotel ;
              rdfs:comment "Type of hotel"@en ;
              rdfs:label "hasHotelType"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasLocationCity
:hasLocationCity rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isLocationOfCity ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain :City ;
                 rdfs:range :Location ;
                 rdfs:comment "Location of city"@en ;
                 rdfs:label "hasCityLocation"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasTypeRestaurant
:hasTypeRestaurant rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isRestaurantTypeOf ;
                   rdfs:domain :Restaurant ;
                   rdfs:range :TypeRestaurant ;
                   rdfs:comment "Type of restaurant"@en ;
                   rdfs:label "hasRestaurantType"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#hasTypeTransport
:hasTypeTransport rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isTransportTypeOf ;
                  rdfs:domain :Transport ;
                  rdfs:range :TypeTransport ;
                  rdfs:comment "Type of transport"@en ;
                  rdfs:label "hasTransportType"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isAttractionTypeOf
:isAttractionTypeOf rdf:type owl:ObjectProperty ;
                    rdfs:domain :TypeAttraction ;
                    rdfs:range :Attraction ;
                    rdfs:comment "inverse of hasAttractionType"@en ;
                    rdfs:label "isAttractionTypeOf"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isEventTypeOf
:isEventTypeOf rdf:type owl:ObjectProperty ;
               rdfs:domain :TypeEvent ;
               rdfs:range :Event ;
               rdfs:comment "inverse of HasEventType"@en ;
               rdfs:label "isEventTypeOf"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isHotelTypeOf
:isHotelTypeOf rdf:type owl:ObjectProperty ;
               rdfs:domain :TypeHotel ;
               rdfs:range :Hotel ;
               rdfs:comment "inverse of hasHotelType"@en ;
               rdfs:label "isHotelTypeOf"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isInCity
:isInCity rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Offers ;
          rdfs:range :City ;
          rdfs:comment "Offer is in city"@en ;
          rdfs:label "isInCity"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isLocationOfCity
:isLocationOfCity rdf:type owl:ObjectProperty ;
                  rdfs:domain :Location ;
                  rdfs:range :City ;
                  rdfs:comment "inverse of hasCityLocation"@en ;
                  rdfs:label "isLocationOfCity"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isRestaurantTypeOf
:isRestaurantTypeOf rdf:type owl:ObjectProperty ;
                    rdfs:domain :TypeRestaurant ;
                    rdfs:range :Restaurant ;
                    rdfs:comment "inverse of hasRestaurantType"@en ;
                    rdfs:label "isRestaurantTypeOf"@en .


###  http://www.semanticweb.org/dgc/ontologies/2025/2/Smart-Tourism-and-Recommendations#isTransportTypeOf
:isTransportTypeOf rdf:type owl:ObjectProperty ;
                   rdfs:domain :TypeTransport ;
                   rdfs:range :Transport ;
                   rdfs:comment "inverse of hasTransportType"@en ;
                   rdfs:label "isTransportTypeOf"@en .


