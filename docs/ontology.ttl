:hasAttractionScore rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Attraction ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:onDatatype xsd:float ;
                                 owl:withRestrictions ( [ xsd:minExclusive "0.0"^^xsd:float
